{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "QuvaConnection": "Data Source=BLACKI/blacki;Connection Timeout=30;User Id=quva;Password=quva;",
    "QusyConnection": "Data Source=blacki/blacki;Connection Timeout=30;User Id=qusy;Password=qusy;"
  },
  "Smtp": {
    "Host": null,
    "Port": null,
    "Ssl": null,
    "User": null,
    "Password": null
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore.Database.Command": "Information",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System": "Warning",
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Information",
        "Microsoft.EntityFrameworkCore.Query": "Error"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "@mt = 'An unhandled exception has occurred while executing the request.'"
        }
      }
    ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "D:/temp/blazor/logs/Test7_log-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}]{ZZZSourceContext} [{IP}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }

}